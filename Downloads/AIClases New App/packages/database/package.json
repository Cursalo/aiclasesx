{
  "name": "@aiclases/database",
  "version": "1.0.0",
  "private": true,
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "db:generate-types": "supabase gen types typescript --project-id xmtjzfnddkuxdertnriq > src/types/database.ts",
    "db:push": "supabase db push",
    "db:reset": "supabase db reset",
    "db:seed": "supabase seed"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.38.0"
  },
  "devDependencies": {
    "typescript": "^5.3.3",
    "supabase": "^1.123.4"
  }
}